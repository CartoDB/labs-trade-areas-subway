              "options": {
                "sql": "WITH iso AS (\n  SELECT\n    the_geom\n  FROM\n  \tnerikcarto.nyc_subway_stations_l_isos\n  WHERE\n  \tdata_range = 1200\n  AND\n  \tstation_id = 94\n  LIMIT 1\n)\n\nselect\n    nerikcarto.nyc_dots_500.cartodb_id,\n    nerikcarto.nyc_dots_500.the_geom,\n    nerikcarto.nyc_dots_500.the_geom_webmercator,\n\tST_Within(\n    \tnerikcarto.nyc_dots_500.the_geom,\n    \t(\n  SELECT\n    the_geom\n  FROM\n  \tnerikcarto.nyc_subway_stations_l_isos\n  WHERE\n  \tdata_range = 1200\n  AND\n  \tstation_id = 94\n  LIMIT 1\n)\n    ) inside\n\nFROM nerikcarto.nyc_dots_500\n    \n",
